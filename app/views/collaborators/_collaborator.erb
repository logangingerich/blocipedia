<% if current_user.admin? || current_user.id == @wiki.user_id %>
  <h2>Add Collaborator?</h2>
  <%= form_for Collaborator.new do |f| %>
    <% User.all.each do |user| %>
      <div class="user">
        <%= user.email %>
      </div>
      <div class="form-group">
        <%= f.submit "Add As Collaborator", class: 'btn btn-success' %>
      </div>
    <% end %>
  <% end %>
<% end %>
